    <script>
        $('.minus').click(function() {
            var $this = $(this);
            var $input = $this.closest('div').find('input');
            var value = parseInt($input.val());

            if (value > 1) {
                value = value - 1;
            } else {
                value = 0;
            }
            $input.val(value);
        });

        $('.plus').click(function() {
            var $this = $(this);
            var $input = $this.closest('div').find('input');
            var value = parseInt($input.val());

            if (value > 0 && value < 100) {
                value = value + 1;
            } else {
                value = 100;
            }
            $input.val(value);

        });
    </script>

